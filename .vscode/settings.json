{
  "deno.enable": true,
  "deno.unstable": true,
  "deno.lint": true,
  "deno.config": "./deno.json",
  "editor.formatOnSave": true,
  "[typescript]": { "editor.defaultFormatter": "denoland.vscode-deno" },
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "prettier.enable": false,
  "deno.documentPreloadLimit": 0,
}
