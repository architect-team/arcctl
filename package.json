{
  "name": "@architect-io/arcctl",
  "version": "0.1.4",
  "description": "arcctl standardizes the interfaces for common cloud resources like VPCs, managed kubernetes clusters, and more, making it easier for developers to create and manage on-demand cloud infrastructure",
  "author": "Architect.io @architect-team",
  "homepage": "https://github.com/architect-team/arcctl",
  "repository": "architect-team/arcctl",
  "type": "module",
  "bin": {
    "arcctl": "./bin/run.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json"
  ],
  "dependencies": {
    "@architect-io/arc-oci": "0.1.0",
    "cli-spinners": "^2.7.0",
    "digitalocean": "^1.2.1",
    "digitalocean-js": "^2.0.3",
    "dots-wrapper": "^3.10.0",
    "log-symbols": "^5.1.0",
    "log-update": "^5.0.1",
    "mongodb": "^4.10.0",
    "mysql2": "^2.3.3",
    "strip-ansi": "^7.0.1",
    "tar": "^6.1.11",
    "untildify": "^4.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@saithodev/semantic-release-backmerge": "^2.1.2",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.0",
    "@types/mustache": "^4.2.1",
    "@types/tar": "^6.1.2",
    "mustache": "^4.2.0",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "ts-xor": "^1.0.8"
  },
  "scripts": {
    "semantic-release": "./node_modules/semantic-release/bin/semantic-release.js",
    "lint": "eslint . --ext .ts --config .eslintrc.cjs",
    "version": "oclif readme && git add README.md",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:service": "ts-node --project ./tsconfig.json ./tests/integration/test-runner.ts",
    "generate:resources": "ts-node --project ./tsconfig.json ./bin/scripts/resource-generator.ts"
  }
}
