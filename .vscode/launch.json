{
    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Deno Debug Template",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "${userHome}/.deno/bin/deno",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--inspect-wait",
                "--allow-all"
            ],
            "args": [],
            "attachSimplePort": 9229
        }
    ]
}